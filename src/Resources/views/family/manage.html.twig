{% trans_default_domain 'UserAdmin' %}

{% extends 'kookaburra.html.twig' %}

{% set module = {'baseURL': 'user_admin', 'crumbs': [{uri: 'user_admin__manage', name: 'People Admin'}], title: 'Manage Families', domain: 'UserAdmin'} %}
{% set breadcrumbs = 'new' %}
{% set moduleName = 'UserAdmin' %}


{% block content %}
    <h3>{{ 'View'|trans }}
        <button class="close-button gray" title="{{ 'Add Person'|trans }}" type="button" onclick="window.open('{{ path('user_admin__family_manage_add') }}','_self')"><span class="fas fa-plus-circle fa-fw fa-1-5x text-gray-700"/></button>
    </h3>
    <section id="paginationContent"></section>
{% endblock content %}
